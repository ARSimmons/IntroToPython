#!/usr/bin/env bash


# # Set aliases
# alias ux='chmod u+x';
# alias ax='chmod a+x';


# Begin work session
function start_work {
	git checkout master;
	git fetch upstream master;
	git merge upstream/master;
	git push;

	# Print when ready to develop
	git status;
	echo;
	echo 'Ready to develop!';
	echo 'Please proceed...';
}

function commit_work {
	git commit -m 'another update';
	git push;
}

# End work session 
function end_work {
	echo 'Pusing git...';
	git push origin master;	
	echo 'Push complete!';
	echo 'Goodbye ~';
}

function just_another {
	echo 'day in the neighborhood'; 
}


# export passed_func;


# source ./start;


